CREATE CONSTRAINT ON (n:Title) ASSERT n.id IS UNIQUE;
CREATE CONSTRAINT ON (n:Title) ASSERT n.titleId IS UNIQUE;
CREATE INDEX ON :Title(originalTitle);
CREATE INDEX ON :Title(primaryTitle);

CREATE CONSTRAINT ON (n:Person) ASSERT n.id IS UNIQUE;
CREATE CONSTRAINT ON (n:Person) ASSERT n.personId IS UNIQUE;
CREATE INDEX ON :Person(name);

CREATE CONSTRAINT ON (n:Genre) ASSERT n.id IS UNIQUE;
CREATE CONSTRAINT ON (n:Genre) ASSERT n.name IS UNIQUE;

